datasource db {
    // provider = "sqlite"
    // url = "file:./database.db"
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-py"
}

model User {
    id Int @id @default(autoincrement())
    username String @unique
    max_score Int @default(0)
}